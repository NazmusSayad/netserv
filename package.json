{
  "name": "netserv",
  "version": "2.0.0",
  "type": "commonjs",
  "bin": "./dist-server/server/index.js",
  "scripts": {
    "dev": "vite --host",
    "tsc": "tsc --watch",
    "serve": "node ./dist-server/server/index.js",
    "server": "nodemon --exec \"ts-node --project tsconfig.server.json -r tsconfig-paths/register ./src/server/index.ts\"",
    "build": "tsc && vite build && npm run server:build",
    "server:build": "tsc -p tsconfig.server.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "qrcode": "^1.5.3",
    "module-alias": "^2.2.3",
    "noarg": "^0.0.11"
  },
  "devDependencies": {
    "tsconfig-paths": "^4.2.0",
    "@reduxjs/toolkit": "^2.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "css-reset-plus": "^1.0.4",
    "error-boundary-react": "^1.0.2",
    "get-classnames": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-net-kit": "^0.0.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-rtk": "^1.0.4",
    "typescript": "^5.3.3",
    "use-effect-except-on-mount": "^0.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.70.0",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0",
    "vite-pwa": "^1.0.2",
    "vite-svg-loader": "^5.1.0"
  },
  "_moduleAliases": {
    "@": "./dist/server"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "last 5 version",
      "> 1%",
      "IE 11"
    ]
  }
}
