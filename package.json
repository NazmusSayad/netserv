    "dev-server": "tsc -p ./server --watch",
    "build": "node ./build.cjs",
    "build-server": "tsc -p ./server",
    "build-web": "tsc -p ./web && vite build ./web --emptyOutDir",
    "lab-run": "node --watch ./dist/server __lab.js",
