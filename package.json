{
  "name": "netserv",
  "version": "3.0.1",
  "type": "commonjs",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "tsc": "tsc --watch --noEmit",
    "web": "ts-node-dev --respawn --rs ./src/index.ts web",
    "ftp": "ts-node-dev --respawn --rs ./src/index.ts ftp",
    "get": "ts-node-dev --respawn --rs ./src/index.ts get",
    "send": "ts-node-dev --respawn --rs ./src/index.ts send 192.168.0.100 8000",
    "vite-dev": "vite ./web --host",
    "vite-build": "tsc -p ./web/tsconfig.json && vite build ./web --emptyOutDir"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "busboy": "^1.6.0",
    "cli-progress": "^3.12.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "formidable": "^3.5.1",
    "ftp-srv": "^2.16.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "noarg": "^1.0.2",
    "portfinder": "^1.0.32",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@mui/styles": "^5.16.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@mui/types": "^7.2.15",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/bunyan": "^1.8.11",
    "@types/busboy": "^1.5.4",
    "@types/cli-progress": "^3.11.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.8",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "css-reset-plus": "^1.0.4",
    "error-boundary-react": "^1.0.2",
    "get-classnames": "^2.0.0",
    "postcss-preset-env": "^9.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-net-kit": "^0.0.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-rtk": "^1.0.4",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "^1.70.0",
    "typescript": "^5.5.2",
    "use-effect-except-on-mount": "^0.0.0",
    "vite": "^5.3.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-pwa": "^1.0.2",
    "vite-svg-loader": "^5.1.0"
  }
}
