    "ftp-run": "node --watch ./dist/server ftp",
    "get-run": "node --watch ./dist/server get",
    "send-run": "node --watch ./dist/server send",
    "lab": "run-p dev-server lab-run",
    "web": "run-p dev-server web-run",
    "ftp": "run-p dev-server ftp-run",
    "get": "run-p dev-server get-run",
    "send": "run-p dev-server send-run"
